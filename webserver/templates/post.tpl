<div class="post" id="post_%%id%%">
	<div class="postinfo">
		<span class="username">%%name%%</span>
		<span class="subject">%%subject%%</span>
		<span class="id" onclick="replyTo(this)">%%id%%</span>
		<span class="posttime">%%time%%</span>
		<span class="pow">%%pow%%</span>
		%%replycount%%
		%%treelink%%
		%%uplink%%
	</div>
	%%filelist%%
	<div class="text">
		%%text%%
		%%modsign%%
	</div>
	%%langlist%%
	%%taglist%%
</div>